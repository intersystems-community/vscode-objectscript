    "pretest": "npm run test-compile",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint src/**",
    "lint-fix": "eslint --fix src/**",
    "download-api": "vscode-dts dev",
    "postdownload-api": "vscode-dts master",
    "postinstall": "npm run download-api"
  },
