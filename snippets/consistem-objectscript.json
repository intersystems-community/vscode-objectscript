            "\tset TABGRID(${1:codGrid},6)=\"; csw:gridCols:cod=${1:codGrid}; Tipo=${23:tipoColuna};\tCsw=${24:Tamanho}^${25:NomeColuna}^;\t\tList=^^;\tCsv=${25:NomeColuna};\"",
            "\tset TABGRID(${1:codGrid},7)=\"; csw:gridCols:cod=${1:codGrid}; Tipo=${26:tipoColuna};\tCsw=${27:Tamanho}^${28:NomeColuna}^;\t\tList=^^;\tCsv=${28:NomeColuna};\"",
            "\t;",
            "\tset sc=\\$\\$Inicializar^%CSW1GRID(CT,%PRG,${1:codGrid},.TABGRID)",
            "\tset sc=\\$\\$Limpar^%CSW1GRID(CT,%PRG,${1:codGrid})",
