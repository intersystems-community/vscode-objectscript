            "\tset TABGRID(${1:codGrid},2)=\"; csw:gridCols:cod=${1:codGrid}; Tipo=${11:tipoColuna};\tCsw=${12:Tamanho}^${13:NomeColuna}^;\t\tList=^^;\tCsv=${13:NomeColuna};\"",
            "\tset TABGRID(${1:codGrid},3)=\"; csw:gridCols:cod=${1:codGrid}; Tipo=${14:tipoColuna};\tCsw=${15:Tamanho}^${16:NomeColuna}^;\t\tList=^^;\tCsv=${16:NomeColuna};\"",
            "\tset TABGRID(${1:codGrid},4)=\"; csw:gridCols:cod=${1:codGrid}; Tipo=${17:tipoColuna};\tCsw=${18:Tamanho}^${19:NomeColuna}^;\t\tList=^^;\tCsv=${19:NomeColuna};\"",
