    "input_consult": {
        "prefix": [
            "inptcst, inputConsult"
        ],
        "body": [
            "\t;",
            "\t; Consultar",
            "\t;",
            "${1:LABEL}\tif '\\$\\$Validate() quit:\\$\\$CSP^%CSW1UTI()",
            "\t;",
            "\tset sc=\\$\\$Inicializar^%CSW1GRID(CT,%PRG,${2:codGrid},.TABGRID)",
            "\tset sc=\\$\\$Limpar^%CSW1GRID(CT,%PRG,${2:codGrid})",
            "\t;",
            "\tdo AG^%CSUTIUD(,\"${1:LABEL}AG1^${TM_FILENAME/(\\w+)(\\.)(\\w+)$/$1/gi}\")",
            "\tquit:\\$\\$CSP^%CSW1UTI()",
            "\t;",
            "${1:LABEL}AG1\tset sc=\\$\\$${3:RegraGeracaoGlobalTrabalho}^${TM_FILENAME/(\\w+)(\\.)(\\w+)$/$1/gi}RG(${4:Parametros})",
            "\tset sc=\\$\\$GerarGrid^${TM_FILENAME/(\\w+)(\\.)(\\w+)$/$1/gi}RG(${5:Parametros})",
            "\t;",
            "\tdo FJ^%CSUTIUD",
            "\tdo FJAG^%CSW1UTI",
            "\t;",
            "\tset sc=\\$\\$ValidarDisplay^%CSW1GRID(CT,%PRG,${2:codGrid})",
            "\tif (sc'=1) do ME^%CSUTICSP(sc) goto ${6:LabelRetorno}",
            "\t;",
            "\tset sc=\\$\\$Movimentar^%CSW1GRID(CT,%PRG,${2:codGrid},,1)",
            "\t;"
        ],
        "description": "Input - Linha para o Grid"
    },
    "input_focus_grid": {
