            "\tquit:\\$\\$CSP^%CSW1UTI()",
            "\t;",
            "ExcluirSN\t;",
            "\tif (SN'=\"S\") goto ${2:labelRetornoNaoExcluir}",
            "\t;",
            "\tset sc=\\$\\$${3:regraDeExclusao}^${TM_FILENAME/(\\w+)(\\.)(\\w+)$/$1/gi}RG(${4:parametrosExclusao})",
            "\tif sc'=1 do ME^%CSUTICSP(sc) goto ${5:labelRetornoErroExcluir}",
            "\t;",
            "\tdo MECABECALHO^%CSW1UTI(\"${6:MensagemExclusao}\")",
            "\t;",
            "\tgoto ${7:labelRetornoExclusaoSucesso}",
            "\t;"
        ],
        "description": "Input - Excluir"
    },
